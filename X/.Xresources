! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #843179, #f5f5f6, #272262
rofi.color-normal: #843179, #f5f5f6, #843179, #272262, #f5f5f6
rofi.color-active: #843179, #f5f5f6, #843179, #272262, #f5f5f6
