bar-list: ["topbar"]

topbar: {
	font: "Serif Sans 10";
	position: "top";
	gap-horizontal: 15;
	gap-verital: 15;
	height: 25;
	slack-size: 1;
	border-size: 1;
	border-color-rgb: 0xf4f4f5
	background-color-rgb: 0x1d232c
	background-color-nowindow-argb: 0xfff79295
	block-list: ["time", "date"]

	time: { 
		exec: "YABAR_DATE";
		align: "right";
		variable-size: true;
		type: "periodic";
		interval: 1;
		internal-prefix: " ";
		internal-option1: "%I:%M:%S";
	}

	date: {
		exec: "date";
		align: "right";
		variable-size: true;
		type: "periodic";
		interval: 1800;
		internal-prefix: " ";
		internal-option2: "%D";
	}
}
